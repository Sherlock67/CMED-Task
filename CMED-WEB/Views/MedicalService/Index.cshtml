 @model CMED_WEB.Models.Root
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<body>
    <div class="text-end">
     <a asp-controller ="Prescription" asp-action="AddPrescription" class="btn btn-primary">
           <i class="bi bi-plus-lg"></i> &nbsp Create New Prescription
       </a>
    </div>
     
    <div class="container">
        <table style="width:100%" class="table table-boredered">
            <thead>
                <tr>
                    <th>
                       sourceDisclaimer
                    </th>
                    <th>
                      SourceName
                    </th>
                    <th>
                       Comment
                    </th>
                    <th>
                      Name
                    </th>
                    <th>
                          Rxcui
                    </th>
                     <th>
                          tty
                    </th>
                    <th>
                          Severity
                    </th>
                    <th>
                        Description
                    </th>
                     <th>
                        MinConceptItem Name
                    </th>
                    
                
                </tr>
            </thead>
            @foreach (var item in Model.interactionTypeGroup)
            {
                <tr>
                    <td>
                        @item.sourceDisclaimer
                    </td>
                    <td>
                        @item.sourceName
                    </td>
                    <td>
                        @item.interactionType[0].comment
                    </td>
                    <td>
                       @item.interactionType[0].minConceptItem.name
        
                    </td>
                    <td>
                         @item.interactionType[0].minConceptItem.rxcui
        
                    </td>
                     <td>
                      @item.interactionType[0].minConceptItem.tty
        
                    </td>
                    <td>
                         @item.interactionType[0].interactionPair[0].severity
                    </td>
                     <td>
                         @item.interactionType[0].interactionPair[0].description
                    </td>
                     <td>
                         @item.interactionType[0].interactionPair[0].interactionConcept[0].minConceptItem.name
                    </td>
                    
                     
                     
                </tr>
        
            }
        
        </table>
    </div>
</body>
</html>
